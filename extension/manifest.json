{
    "manifest_version": 2,
    "name": "Castle Age Monster Code Opener",
    "short_name": "CAMCO",
    "version": "0.1.0.0",
    "description": "This extension converts monster code of CA to monster URL of web3 or facebook page, and opens it for you.",
    "default_locale": "en",
    "background": {
        "page": "background.html"
    },  
    "permissions": [
      "contextMenus",
      "clipboardWrite"
    ],
	"icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
	}
}

